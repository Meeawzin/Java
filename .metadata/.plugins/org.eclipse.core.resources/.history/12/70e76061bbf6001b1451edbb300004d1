import java.util.Locale;

public class main {

	public static void main(String[] args) {
		
		
		System.out.println("hello world!");
		
		int x = 1;
		
		Locale.setDefault(Locale.US);
		
		double y = 2.2345;
		
		System.out.printf("%.2f%n", y);
		
		
		double z = 3.5678;
		
		System.out.printf("RESULTADO = " + z + " metros");
		
		
		String q = "Maria";
		int w = 26;
		double e = 3500.0;
		
		System.out.printf("%s tem %d e recebe $ %.2f%n", q , w, e);
		
		
		
		
		
		
		
	}

}
